<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Marlinc\AdminBundle\Model\TrashManager" alias="marlinc.admin.trash.manager" />
        <service id="marlinc.admin.trash.orm.reader" class="Marlinc\AdminBundle\Model\TrashReader" public="true">
            <argument type="service" id="doctrine.orm.entity_manager" />
            <argument type="service" id="stof_doctrine_extensions.listener.softdeleteable" />
        </service>
    </services>
</container>
